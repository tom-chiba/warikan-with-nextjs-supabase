{
  "permissions": {
    "allow": [
      "Edit",
      "MultiEdit",
      "WebFetch",
      "WebSearch",
      "Write",
      "Bash(ls:*)",
      "Bash(pwd:*)",
      "Bash(grep:*)",
      "Bash(* | grep:*)",
      "Bash(npm run tsc)",
      "Bash(npm run biome:check)",
      "Bash(npm run biome:format)",
      "Bash(npm run check)",
      "Bash(npm run test:comp:*)",
      "Bash(npm run test:e2e:*)",
      "Bash(mkdir:*)",
      "Bash(git status:*)",
      "Bash(git log:*)",
      "Bash(git show:*)",
      "Bash(git diff:*)",
      "Bash(git grep:*)",
      "Bash(git branch)",
      "Bash(git remote)"
    ],
    "deny": [
      "Bash(git push:*)",
      "Bash(git clean:*)",
      "Bash(git stash drop:*)",
      "Bash(git stash clear:*)",
      "Bash(git reset --hard:*)",
      "Bash(git filter-branch:*)",
      "Bash(git filter-repo:*)",
      "Bash(git branch -D:*)",
      "Bash(git rebase:*)"
    ]
  }
